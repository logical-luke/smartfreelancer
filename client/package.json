{
  "name": "smartfreelancer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "node_modules/.bin/vite build",
    "lint": "node_modules/.bin/eslint . --fix",
    "css": "npm-run-all --sequential css-*",
    "css-compile": "node_modules/.bin/tailwindcss build -i src/tailwind/tailwind.css -c tailwind.config.js -o public/css/tailwind/tailwind.css",
    "css-minified": "node_modules/.bin/tailwindcss build -i src/tailwind/tailwind.css -c tailwind.config.js -o public/css/tailwind/tailwind.min.css --minify",
    "dev": "node_modules/.bin/vite --host 0.0.0.0",
    "preview": "node_modules/.bin/vite preview",
    "translate": "node tools/translator.js",
    "vue-i18n-extract": "node_modules/.bin/vue-i18n-extract --add true --remove true --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/locale/*.?(json|yml|yaml|js)' --noEmptyTranslation 'en'",
    "watch-css": "node_modules/.bin/tailwindcss build -i src/tailwind/tailwind.css -c tailwind.config.js -o public/css/tailwind/tailwind.min.css --minify --watch"
  },
  "dependencies": {
    "@eslint/js": "^9.10.0",
    "@primevue/themes": "^4.0.7",
    "axios": "^1.2.2",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "pinia": "^2.2.*",
    "primeicons": "^7.*",
    "primevue": "^4.*",
    "tailwind-merge": "^1.9.1",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.5.*",
    "vue-class-component": "^8.0.0-0",
    "vue-cookies": "^1.8.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-simple-password-meter": "^1.1.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^9.10.*",
    "eslint-plugin-vue": "^9.3.*",
    "postcss": "^8.4.*",
    "prettier": "^2.8.*",
    "tailwindcss": "^3.4.*",
    "typescript": "~4.5.5",
    "vite": "^5.0.*",
    "vite-plugin-environment": "^1.1.3",
    "vue-i18n-extract": "^2.0.7"
  }
}
