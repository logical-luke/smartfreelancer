{
  "name": "smartfreelancer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node_modules/.bin/vite",
    "build": "node_modules/.bin/vite build",
    "preview": "node_modules/.bin/vite preview",
    "lint": "node_modules/.bin/eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "watch-css": "node_modules/.bin/tailwindcss build -i src/tailwind/tailwind.css -c src/tailwind/tailwind.config.js -o public/css/tailwind/tailwind.min.css --minify --watch",
    "css-compile": "node_modules/.bin/tailwindcss build -i src/tailwind/tailwind.css -c src/tailwind/tailwind.config.js -o public/css/tailwind/tailwind.css",
    "css-minified": "node_modules/.bin/tailwindcss build -i src/tailwind/tailwind.css -c src/tailwind/tailwind.config.js -o public/css/tailwind/tailwind.min.css --minify",
    "css": "npm-run-all --sequential css-*"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "vite-plugin-environment": "^1.1.3",
    "vue": "^3.2.45",
    "vue-cookies": "^1.8.2",
    "vue-router": "^4.1.6",
    "vue-select": "^4.0.0-beta.6",
    "vue-tabler-icons": "^2.17.0",
    "vue3-acies-treeselect": "^0.1.3",
    "vue3-spinner": "^0.0.16",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.0"
  }
}
